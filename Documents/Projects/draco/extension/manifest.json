{
    "manifest_version": 2,
    "name": "Draco",
    "description": "Injects custom code to [website]",
    "version": "1",
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}